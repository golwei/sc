              <swiper-item>
                <image src="{{item.src}}" class="img" />
              </swiper-item>
            </block>
          </swiper>

          <view class="dots">
            <block wx:for="{{items4}}" wx:key="unique">
              <view class="dot{{index == swiperCurrent ? ' active' : ''}}"></view>
            </block>
          </view>
        </view>
      </view>
    </view>
